{
    "name": "Voice Music Bot",
    "logo": "https://telegra.ph/file/bad69dc7929731b11e056.jpg",
    "description": "Cyber Music project allow you to stream music trought the telegram voice chat feature.",
    "keywords": ["music", "voice chat", "telegram"],
    "repository": "https://github.com/Mehmet5506/TaliaMusic99",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "pyrogram oturum dizesiyle doldurma",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "bot jetonunuz @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "müzik botu adınız.",
            "required": false,
            "value": ""
        },
        "BOT_USERNAME": {
            "description": "bot kullanıcı adınızla doldurmadan @",
            "required": true,
            "value": ""
        },
        "ASSISTANT_NAME": {
            "description": "yardımcı kullanıcı adı ile doldurma @",
            "required": true,
            "value": ""
        },
        "BG_IMAGE": {
            "description": "fill with your costum background image link",
            "required": false,
            "value": "https://i.ibb.co/3mB3sKc/288e6c40a049f4c9b3639bd89e0c9f8e.jpg"
        },
        "THUMB_IMG": {
            "description": "fill with costum thumbnail image it's different with bg_image",
            "required": false,
            "value": ""
        },
        "OWNER_NAME": {
            "description": "kullanıcı adınızla doldurmadan @",
            "required": false,
            "value": ""
        },
        "API_ID": {
            "description": "uygulama kimliğinizden.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "benim api karma.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "sudo üye listesi listesi kimliği (Boşluklara göre ayır).",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "max durasi untuk video yang diunduh (dakikalar içinde).",
            "required": true,
            "value": "60"
        },
        "PMPERMIT": {
            "description": "pm-permit for assistant.",
            "required": false,
            "value": "ENABLE"
        },
        "UPDATES_CHANNEL": {
            "description": "Kanal kullanıcı adınız ile doldurun, Eğer iznin yoksa.",
            "required": false,
            "value": ""
        },
        "GROUP_SUPPORT": {
            "description": "isi dengan username grup kamu, Atlamanız yoksa",
            "required": false,
            "value": ""
        }
    },
    "buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
    }
  ]
}
